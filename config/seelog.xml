<seelog>
    <outputs formatid="main">
        <filter levels="info,critical,error">
            <console />
        </filter>
        <filter levels="debug, error">
            <file path="./log/App.log" />
        </filter>
    </outputs>
    <formats>
        <format id="main" format="%Date/%Time [%LEV] %Msg%n"/>
    </formats>
</seelog>